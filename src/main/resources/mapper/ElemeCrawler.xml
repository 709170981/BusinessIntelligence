<mapper namespace="com.business.intelligence">
    <insert id="insertEvaluate" keyProperty="id" parameterType="ElemeEvaluate">
        insert into eleme_evaluate(shopId,date,evaValue,star,goods,num,type) values (#{shopId},#{date},#{evaValue},#{star},#{goods},#{num},#{type})
