<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.business.intelligence.model.TUser">
    <select id="selectUserById" parameterType="int" resultType="TUser">
        select * from User where id = #{id}
